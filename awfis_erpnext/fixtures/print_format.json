  "html": "<div class=\"page-break\">\n    <div class=\"print-heading\">\n        <h2>Purchase Order<br>\n            <small>{{doc.name}}</small>\n        </h2>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6 column-break\">\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>Supplier Name</label>\n                    </div>\n                    <div class=\"col-xs-7  value\">\n                        {{doc.supplier_name}}\n                    </div>\n                </div>\n                {% if doc.address_display %}\n                    <div class=\"row\">\n                        <div class=\"col-xs-5 text-right\">\n                            <label>Address</label>\n                        </div>\n                        <div class=\"col-xs-7  value\">\n                            {{doc.address_display}}\n                        </div>\n                    </div>\n                {%- endif -%}\n                {% if doc.contact_display %}\n                    <div class=\"row\">\n                        <div class=\"col-xs-5 text-right\">\n                            <label>Contact</label>\n                        </div>\n                        <div class=\"col-xs-7  value\">\n                            {{doc.contact_display}}\n                        </div>\n                    </div>\n                {%- endif -%}\n                {% if doc.contact_mobile %}\n                    <div class=\"row\">\n                        <div class=\"col-xs-5 text-right\">\n                            <label>Mobile No</label>\n                        </div>\n                        <div class=\"col-xs-7  value\">\n                            {{doc.contact_mobile}}\n                        </div>\n                    </div>\n                {%- endif -%}\n\n        </div>\n        \n        <div class=\"col-xs-6 column-break\">\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Date</label>\n                </div>\n                <div class=\"col-xs-7  value\">\n                    {{doc.transaction_date}}\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-right\">\n                    <label>Customer Name</label>\n                </div>\n                <div class=\"col-xs-7  value\">\n                    {{doc.customer_name}}\n                </div>\n            </div>\n            {% if doc.shipping_address_display %}\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>Customer Address</label>\n                    </div>\n                    <div class=\"col-xs-7  value\">\n                        {{doc.shipping_address_display}}\n                    </div>\n                </div>\n            {%- endif -%}\n            {% if doc.customer_contact_display %}\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>Customer Contact</label>\n                    </div>\n                    <div class=\"col-xs-7  value\">\n                        {{doc.customer_contact_display}}\n                    </div>\n                </div>\n            {%- endif -%}\n            {% if doc.customer_contact_mobile %}\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>Customer Mobile No</label>\n                    </div>\n                    <div class=\"col-xs-7  value\">\n                        {{doc.customer_contact_mobile}}\n                    </div>\n                </div>\n            {%- endif -%}\n        </div>\n    </div>\n    <table class=\"table table-bordered\">\n        <tbody>\n            <tr>\n                <th>Sr</th>\n                <th>Item Code</th>\n                <th>Description</th>\n                <th class=\"text-right\">Qty</th>\n                <th class=\"text-right\">Rate</th>\n                <th class=\"text-right\">Amount</th>\n            </tr>\n            {%- for row in doc.items -%}\n            <tr>\n                <td style=\"width: 3%;\">{{ row.idx }}</td>\n                <td style=\"width: 20%;\">\n                    {{ row.item_code }}\n                </td>\n               <td style=\"width: 37%;\">\n               \t\t<strong>{{ row.item_name }}</strong><br>\n                    {{ row.description }}<br>\n\t\t\t\t\tWarehouse: {{row.warehouse}}<br>\n\t\t\t\t\t{{ frappe.db.get_value(\"Warehouse\",row.warehouse,\"address_line_1\") or \"\"}}\n\t\t\t\t\t{% if frappe.db.get_value(\"Warehouse\",row.warehouse,\"address_line_2\") %}\n\t\t\t\t\t{{ \", \" + frappe.db.get_value(\"Warehouse\",row.warehouse,\"address_line_2\")}}<br>\n\t\t\t\t\t{% endif %}\n\t\t\t\t\t{% if frappe.db.get_value(\"Warehouse\",row.warehouse,\"city\") %}\n\t\t\t\t\t{{ frappe.db.get_value(\"Warehouse\",row.warehouse,\"city\")}}, \n\t\t\t\t\t{% endif %}\n\t\t\t\t\t{% if frappe.db.get_value(\"Warehouse\",row.warehouse,\"state\") %}\n\t\t\t\t\t{{ \", \" + frappe.db.get_value(\"Warehouse\",row.warehouse,\"state\")}}<br>\n\t\t\t\t\t{% endif %}\n\t\t\t\t\t{% if frappe.db.get_value(\"Warehouse\",row.warehouse,\"pin\") != \"0\"  %}\n\t\t\t\t\t{{ frappe.db.get_value(\"Warehouse\",row.warehouse,\"pin\")}}<br>\n\t\t\t\t\t{% endif %}\n\t\t\t\t</td>\n                <td style=\"width: 10%; text-align: right;\">{{ row.qty }} {{ row.uom or row.stock_uom }}</td>\n                <td style=\"width: 15%; text-align: right;\">{{\n                    row.get_formatted(\"rate\", doc) }}</td>\n                <td style=\"width: 15%; text-align: right;\">{{\n                    row.get_formatted(\"amount\", doc) }}</td>\n            </tr>\n            {%- endfor -%}\n        </tbody>\n    </table>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6 column-break\">\n        </div>\n        <div class=\"col-xs-6 column-break\">\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>Total</label>\n                    </div>\n                    <div class=\"col-xs-7 text-right value\">\n                        {{doc.total}}\n                    </div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>Grand Total</label>\n                    </div>\n                    <div class=\"col-xs-7 text-right value\">\n                        {{doc.grand_total}}\n                    </div>\n                </div>\n                <div class=\"row\">\n                    <div class=\"col-xs-5 text-right\">\n                        <label>In Words</label>\n                    </div>\n                    <div class=\"col-xs-7 text-right value\">\n                        {{doc.in_words}}\n                    </div>\n                </div>\n        </div>\n    </div>\n</div>", 
